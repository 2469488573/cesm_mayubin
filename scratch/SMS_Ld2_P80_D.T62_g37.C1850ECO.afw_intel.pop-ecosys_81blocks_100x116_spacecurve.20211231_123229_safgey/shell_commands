./xmlchange --force POP_TAVG_R8=TRUE
# Set number of blocks to 81, use space curve
./xmlchange --force --force POP_AUTO_DECOMP=FALSE
./xmlchange --force --force POP_DECOMPTYPE=spacecurve
./xmlchange --force --force POP_BLCKX=12,POP_BLCKY=14,POP_NX_BLOCKS=9,POP_NY_BLOCKS=9
./xmlchange --force --force POP_MXBLCKS=5
